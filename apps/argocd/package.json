{
  "name": "librepod-apps",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "build": "bun run compile && bun run test && bun run synth",
    "upgrade": "bun i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "bun i cdk8s@next cdk8s-cli@next"
  },
  "dependencies": {
    "@repo/cdk8s-imports": "*",
    "cdk8s": "2.68.33",
    "cdk8s-plus-26": "2.18.78",
    "constructs": "10.3.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/jest-config": "*",
    "@repo/typescript-config": "*",
    "@types/jest": "26",
    "@types/node": "14",
    "cdk8s-cli": "2.198.39",
    "jest": "26",
    "ts-jest": "26",
    "ts-node": "10",
    "typescript": "4.9.5"
  }
}
